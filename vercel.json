{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { 
        "distDir": "build",
        "buildCommand": "npm run vercel-build"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/data/(.*)", "dest": "/data/$1" },
    { "src": "/(.*\\.(json|svg|jpg|png|ico|woff|woff2))", "dest": "/$1" },
    { "src": "/.*", "dest": "/index.html" }
  ]
} 